<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.project1"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="16" />

    <uses-permission android:name="android.permission.WRITE_SETTINGS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.project1.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.project1.LevelActivity"
            android:label="@string/title_activity_level" >
        </activity>
        <activity
            android:name="com.example.project1.BrightnessActivity"
            android:label="@string/title_activity_brightness" >
        </activity>
        <activity
            android:name="com.example.project1.PrimaryActivity"
            android:label="@string/title_activity_primary" >
        </activity>
        <activity
            android:name="com.example.project1.ExpertActivity"
            android:label="@string/title_activity_expert" >
        </activity>
    </application>

</manifest>
