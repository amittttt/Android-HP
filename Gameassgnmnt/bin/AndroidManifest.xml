<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.game"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.game.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.game.StartPage"
            android:label="@string/title_activity_start_page" >
            <intent-filter >
                <action android:name="gamemenu"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.game.GamePage"
            android:label="@string/title_activity_game_page" >
        </activity>
        <activity
            android:name="com.example.game.Game"
            android:label="@string/title_activity_game" >
        </activity>

        <service android:name="com.example.game.Music" >
        </service>

        <activity
            android:name="com.example.game.NewGame"
            android:label="@string/title_activity_new_game" >
        </activity>
        <activity
            android:name="com.example.game.Rules"
            android:label="@string/title_activity_rules" >
        </activity>
        <activity
            android:name="com.example.game.HighScore"
            android:label="@string/title_activity_high_score" >
        </activity>
        <activity
            android:name="com.example.game.Settings"
            android:label="@string/title_activity_settings" >
        </activity>
    </application>

</manifest>
